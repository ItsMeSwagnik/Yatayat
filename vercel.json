{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin-allow-popups"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "unsafe-none"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/vehicles",
      "destination": "/api/consolidated?endpoint=vehicles"
    },
    {
      "source": "/api/google-users",
      "destination": "/api/consolidated?endpoint=google-users"
    },
    {
      "source": "/api/google-vehicles",
      "destination": "/api/consolidated?endpoint=google-vehicles"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ]
}